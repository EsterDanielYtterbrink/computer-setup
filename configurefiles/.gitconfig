[user]
	email = esterdaniel@ytterbrink.com
	name = Ester Daniel Ytterbrink
[rebase]
	instructionFormat = (%an <%ae>) %s
[alias]
  ca = commit -a
  co = checkout
  oops = commit -a --amend
  rb = rebase --i
  st = status
  wip = commit -a -m 'WIP'
  review = diff --name-only master 
	l = log  --decorate=short --pretty=format:'%C(green)%d%C(red) %aN%Creset%n' --graph --all;
	ll = log  --decorate=short --pretty=format:'%C(blue)%h%C(green)%d%C(red) %aN%Creset (%cr)%n        %s%n ' --graph --all;
	k = log --graph --decorate --pretty=oneline --abbrev-commit;
	deps = submodule foreach 'git pull';

[diff]
        color = auto

[pager]
        color = true

[httpd]
    sslVersion = tlsv1.2

[status]
        color = auto
[push]
default = tracking
[core]
	editor = vim
